rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to partner images
    // Allow write access for uploading images (protected by admin auth at app level)
    match /partners/{allPaths=**} {
      allow read: if true;
      allow write: if true;
    }
    
    // Deny access to all other paths
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
